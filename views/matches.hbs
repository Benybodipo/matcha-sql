{{#if isEmpty}}
	<h3>No Matches found</h3>
{{else}}
	{{#each matches}}
		<div class="user">	
			<img src="{{image}}" alt="">
			<br>
			<strong>{{username}}</strong>
			<br>
			<a href="/user/{{user_id}}" class="" style="display: inline-block; margin: 0 5px">
				<i class="fas fa-eye"></i>
			</a>
			<a href="/chat/{{user_id}}" class="">
				<i class="fas fa-comments"></i>
			</a>
		</div>
	{{/each}}
{{/if}}

