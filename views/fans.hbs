{{#if isEmpty}}
	<h3>No Fans</h3>
{{else}}
	{{#each fans}}
		<div class="user">	
			<img src="{{images}}" alt="">
			<br>
			<strong>{{username}}</strong>
			<br>
			<a href="/user/{{_id}}" class="" style="display: inline-block; margin: 0 5px">
				<i class="fas fa-eye"></i>
			</a>
		</div>
	{{/each}}
{{/if}}

