{{#if isEmpty}}
	<h3>No visitors</h3>
{{else}}
	{{#each visitors}}
		<div class="user">	
			<img src="{{image}}" alt="">
			<br>
			<strong>{{username}}</strong>
			<br>
			<a href="/user/{{user_id}}" class="btn" style="display: inline-block; margin: 0 5px">
				MORE
			</a>
		</div>
	{{/each}}
{{/if}}
